{# Leaf and Spine route-maps for the underlay#}
route_maps:
  RM-CONN-2-BGP:
    sequence_numbers:
      10:
        type: permit
        match:
          - "ip address prefix-list PL-LOOPBACKS-EVPN-OVERLAY"
{% if type == "l3leaf" and l2leaf_inband_management_subnet is arista.avd.defined %}
      20:
        type: permit
        match:
          - "ip address prefix-list PL-L2LEAF-INBAND-MGMT"
{% endif %}
