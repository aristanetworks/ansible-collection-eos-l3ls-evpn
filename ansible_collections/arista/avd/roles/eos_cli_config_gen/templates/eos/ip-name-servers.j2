{# eos - ip name servers #}
{% for node in ip_name_servers.nodes | arista.avd.natural_sort %}
{%     if ip_name_servers.source.vrf is arista.avd.defined %}
ip name-server vrf {{ ip_name_servers.source.vrf }} {{ node }}
{%     else %}
ip name-server {{ node }}
{%     endif %}
{% endfor %}
