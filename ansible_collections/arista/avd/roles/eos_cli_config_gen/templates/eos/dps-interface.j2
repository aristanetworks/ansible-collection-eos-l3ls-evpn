{# j2lint: disable=V1 #}
{# eos - dps interface #}
{% if dps_interface.Dps1 is arista.avd.defined %}
!
interface Dps1
{%     if dps_interface.Dps1.description is arista.avd.defined %}
   description {{ dps_interface.Dps1.description }}
{%     endif %}
{%     if dps_interface.Dps1.flow_tracker.hardware is arista.avd.defined %}
   flow tracker hardware {{ dps_interface.Dps1.flow_tracker.hardware }}
{%     endif %}
{%     if dps_interface.Dps1.flow_tracker.sampled is arista.avd.defined %}
   flow tracker sampled {{ dps_interface.Dps1.flow_tracker.sampled }}
{%     endif %}
{%     if dps_interface.Dps1.ip_address is arista.avd.defined %}
   ip address {{ dps_interface.Dps1.ip_address }}
{%     endif %}
{%     if dps_interface.Dps1.tcp_mss_ceiling is arista.avd.defined %}
{%         set tcp_mss_ceiling_cli = "tcp mss ceiling" %}
{%         if dps_interface.Dps1.tcp_mss_ceiling.ipv4 is arista.avd.defined %}
{%             set tcp_mss_ceiling_cli = tcp_mss_ceiling_cli ~ " ipv4 " ~ dps_interface.Dps1.tcp_mss_ceiling.ipv4 %}
{%         endif %}
{%         if dps_interface.Dps1.tcp_mss_ceiling.ipv6 is arista.avd.defined %}
{%             set tcp_mss_ceiling_cli = tcp_mss_ceiling_cli ~ " ipv6 " ~ dps_interface.Dps1.tcp_mss_ceiling.ipv6 %}
{%         endif %}
{%         if dps_interface.Dps1.tcp_mss_ceiling.direction is arista.avd.defined %}
{%             set tcp_mss_ceiling_cli = tcp_mss_ceiling_cli ~ " " ~ dps_interface.Dps1.tcp_mss_ceiling.direction %}
{%         endif %}
   {{ tcp_mss_ceiling_cli }}
{%     endif %}
{%     if  dps_interface.Dps1.eos_cli is arista.avd.defined %}
   {{ dps_interface.Dps1.eos_cli | indent(3, false) }}
{%     endif %}
{% endif %}
