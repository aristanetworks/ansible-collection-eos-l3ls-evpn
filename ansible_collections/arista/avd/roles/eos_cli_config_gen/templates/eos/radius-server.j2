{%     if radius_server.attribute_32_include_in_access_req is arista.avd.defined %}
{%         set attribute_32_include_in_access_cli = "radius-server attribute 32 include-in-access-req" %}
{%         if radius_server.attribute_32_include_in_access_req.hostname is arista.avd.defined (true) %}
{%             set attribute_32_include_in_access_cli = attribute_32_include_in_access_cli ~ " hostname" %}
{%         endif %}
{%         if radius_server.attribute_32_include_in_access_req.format is arista.avd.defined and not radius_server.attribute_32_include_in_access_req.hostname is arista.avd.defined %}
{%             set attribute_32_include_in_access_cli = attribute_32_include_in_access_cli ~ " format " ~ radius_server.attribute_32_include_in_access.format %}
{%         endif %}
{{ attribute_32_include_in_access_cli }}
{%     endif %}
{%     if radius_server.dynamic_authorization is arista.avd.defined %}
{%         set dynamic_authorization_cli = "radius-server dynamic-authorization" %}
{%         if radius_server.dynamic_authorization.port is arista.avd.defined %}
{%             set attribute_32_include_in_access_port_cli = dynamic_authorization_cli ~ " port " ~ radius_server.dynamic_authorization.port %}
{{ attribute_32_include_in_access_port_cli }}
{%         endif %}
{%         if radius_server.dynamic_authorization.tls_ssl_profile is arista.avd.defined %}
{%             set dynamic_authorization_cli = dynamic_authorization_cli ~ " tls ssl-profile " ~ radius_server.dynamic_authorization.tls_ssl_profile %}
{{ dynamic_authorization_cli }}
{%         endif %}
{%     endif %}
{%     if radius_server.hosts is arista.avd.defined %}
{%         for radius_servers in radius_server.hosts %}
{%             if radius_servers.host is arista.avd.defined %}
{%                 set radius_cli = "radius-server host " ~ radius_servers.host %}
{%             endif %}
{%             if radius_servers.vrf is arista.avd.defined %}
{%                 set radius_cli = radius_cli ~ " vrf " ~ radius_servers.vrf %}
{%             endif %}
{%             if radius_servers.timeout is arista.avd.defined %}
{%                 set radius_cli = radius_cli ~ " timeout " ~ radius_servers.timeout %}
{%             endif %}
{%             if radius_servers.retransmit is arista.avd.defined %}
{%                 set radius_cli = radius_cli ~ " retransmit " ~ radius_servers.retransmit %}
{%             endif %}
{%             if radius_servers.key is arista.avd.defined %}
{%                 set radius_cli = radius_cli ~ " key 7 " ~ radius_servers.key %}
{%             endif %}
{{ radius_cli }}
{%         endfor %}
{%     endif %}
{% endif %}
